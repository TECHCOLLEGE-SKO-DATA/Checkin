<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        xmlns:vm="using:CheckInSystemAvalonia.ViewModels"
		x:DataType="vm:Windows.FakeNFCWindowViewModel"
		x:Class="CheckInSystemAvalonia.Views.FakeNFCWindow"
        Title="FakeNFCWindow"
		Icon="/Assets/avalonia-logo.ico">
	
	<Grid>
		<ContentControl Content="{Binding CurrentViewModel}" ContentTemplate="{DynamicResource ViewLocator}"/>
	</Grid>
</Window>
